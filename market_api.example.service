# Rename to market_api.service
[Unit]
Description=Matket_API
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/home/new_bot/market_api
ExecStart=/home/new_bot/market_api/venv/bin/python -m app
KillMode=process
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target


